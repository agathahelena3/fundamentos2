package fundamentos2;

import java.util.Scanner;

public class atv10 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int notas []= new int [5];
				
		for (int i=0; i <= notas.length;i++) {
		System.out.printf("Digite a nota %d dos alunos: ",i=1);
		notas[i]=sc.nextInt();
	}

}}
